CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ONTOLOGY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,ONTTYPE INTEGER NOT NULL,ONTURI VARCHAR NOT NULL,ONTVERSION INTEGER NOT NULL,ONTDESCRIPTION VARCHAR)
CREATE MEMORY TABLE ONTRELATION(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,ONTINTERCHANGE INTEGER NOT NULL,ONTNATIVE INTEGER NOT NULL,ONTFILENAME VARCHAR NOT NULL,CONSTRAINT ONTOLOGY_ONTRELATION_ID_FK1 FOREIGN KEY(ONTINTERCHANGE) REFERENCES ONTOLOGY(ID),CONSTRAINT ONTOLOGY_ONTRELATION_ID_FK2 FOREIGN KEY(ONTNATIVE) REFERENCES ONTOLOGY(ID))
CREATE MEMORY TABLE ONTTRANSLATION(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,ONTRELID INTEGER NOT NULL,FROMCONCEPT VARCHAR NOT NULL,TOCONCEPT VARCHAR NOT NULL,CONSTRAINT ONTOLOGY_ONTTRANSLATION_ID_FK1 FOREIGN KEY(ONTRELID) REFERENCES ONTRELATION(ID))
ALTER TABLE ONTOLOGY ALTER COLUMN ID RESTART WITH 0
ALTER TABLE ONTRELATION ALTER COLUMN ID RESTART WITH 0
ALTER TABLE ONTTRANSLATION ALTER COLUMN ID RESTART WITH 0
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
