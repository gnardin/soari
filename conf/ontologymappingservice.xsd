 <xsd:annotation>
   <xsd:documentation xml:lang="en">
     Ontology Mapping Service Configuration XML schema
   </xsd:documentation>
 </xsd:annotation>
 
 <xsd:element name="OntologyMappingService" type="oms:OntologyMappingServiceType"/>

 <xsd:complexType name="OntologyMappingServiceType">
